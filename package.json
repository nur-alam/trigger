{
	"name": "trigger",
	"version": "1.0.0",
	"description": "Email logging app",
	"main": "index.js",
	"scripts": {
		"dev": "webpack --mode development --watch",
		"mix": "npx mix watch",
		"watch": "concurrently \"npm run dev\" \"npm run mix\"",
		"prod": "webpack --mode production",
		"make-pot": "wp i18n make-pot --exclude='node_modules, vendor' . languages/trigger.pot",
		"bundle": "wp dist-archive . ../trigger-$(grep 'Version:' triggermail.php | awk '{print $3}').zip",
		"build": "composer i --no-dev && npm run prod && npx mix --production && npm run make-pot && npm run bundle"
	},
	"author": "nurwp",
	"license": "ISC",
